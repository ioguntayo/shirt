    "def nearest_square(num):\n",
    "    root = 0\n",
    "    while (root + 1) ** 2 <= num:\n",
    "        root += 1\n",
    "    return root ** 2a\n",
    "\n",
    "\n",
    "nearest_square(22)\n"
   ]
